{
    "name":"tenderplus/yii2-graphql",
    "description":"facebook graphql server side for yii2 php framework",
    "keyword":["yii2","graphql"],    
    "type":"yii2-extension",
    "license": "BSD-3-Clause",
    "authors":[
        {
            "name":"tenderplus",
            "email":"dev@tenderplus.kz"
        }
    ],
    "minimum-stability":"dev",
    "require":{
        "php": ">=7.2",
        "yiisoft/yii2":"~2.0.10",
        "webonyx/graphql-php": "^15.9.0",
        "ecodev/graphql-upload": "^7.0.0",
        "laminas/laminas-diactoros": "^2.26.0"
    },
    "require-dev":{
        "phpunit/phpunit": "4.8.*"
    },
    "autoload":{
        "psr-4":{
            "yii\\graphql\\":"src"
        }
    },
    "archive": {
        "exclude": [
            "/tests"
        ]
    }
}
